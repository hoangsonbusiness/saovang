class NavbarComponent {
    constructor(authService, service, http, router) {
        this.authService = authService;
        this.service = service;
        this.http = http;
        this.router = router;
        this.uri = 'http://localhost:8080/login';
        this.isLogin = false;
        this.data = [];
        this.jwt = '';
        // this.cookieValue = cookieService.get('jwt')
        // console.log('cookieValue = '+this.cookieValue)
        this.jwt = localStorage.getItem('jwt');
    }
    ngOnInit() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            this.isLogin = yield this.isloggedIn();
            this.getData();
        });
    }
    checkLogin() {
        let headers = {
            'jwt': this.jwt
        };
        return this.http.get(`${this.uri}/isLoggin`, { headers: headers })
            .toPromise().then(data => true).catch(err => false);
    }
    isloggedIn() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            if (yield this.checkLogin())
                return true;
            return false;
        });
    }
    getData() {
        this.service.getData()
            .subscribe((data) => {
            this.data = data;
        });
    }
    onLogoutClick() {
        this.authService.logout();
        this.router.navigate(['/dang-nhap']);
        return false;
    }
