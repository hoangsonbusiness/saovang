const uploadAPI = 'http://localhost:8080/file/singleUpload'; // better use a service class
class CourseManageAddComponent {
    constructor(fb, route, service) {
        this.fb = fb;
        this.route = route;
        this.service = service;
        this.fileNameArr = [];
        this.editor1 = "";
        this.editor2 = "";
        this.thumpbnail = "";
        this.uploader = new ng2_file_upload__WEBPACK_IMPORTED_MODULE_3__["FileUploader"]({ url: uploadAPI, itemAlias: 'file' });
    }
    ngOnInit() {
        this.form = this.fb.group({
            title: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_2__["Validators"].required]
        });
        this.uploader.onAfterAddingFile = (file) => { file.withCredentials = false; };
        this.uploader.onCompleteItem = (item, response, status, headers) => {
            response = JSON.parse(response);
            let filename = response.filename;
            if (filename != "") {
                let obj = {
                    filename: filename,
                    orginalFileName: response.orginalFileName
                };
                this.fileNameArr.push(obj);
            }
            //  alert('Your file has been uploaded successfully');
        };
    }
    addData(title) {
        this.editor1 = this.editor1.replace("</html>", "")
            .replace("</title>", "")
            .replace("</head>", "")
            .replace("</body>", "")
            .replace("<html>", "")
            .replace("<title>", "")
            .replace("<head>", "")
            .replace("<body>", "").trim();
        this.editor2 = this.editor2.replace("</html>", "")
            .replace("</title>", "")
            .replace("</head>", "")
            .replace("</body>", "")
            .replace("<html>", "")
            .replace("<title>", "")
            .replace("<head>", "")
            .replace("<body>", "").trim();
        this.service.add(title, this.editor1, this.editor2, this.thumpbnail, this.fileNameArr)
            .subscribe(res => {
            alert('Đăng khóa học mới thành công !');
            this.route.navigateByUrl('/quan-ly/khoa-hoc/danh-sach');
        });
    }
    deleteFile(path) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            let filename = yield this.service.deleteFile(path);
            if (filename != '') {
                let index = 0;
                index = filename.lastIndexOf("/");
                filename = filename.substring(index + 1, filename.length);
            }
            this.fileNameArr = yield this.fileNameArr.filter(function (e) { return e.filename !== filename; });
            alert("Delete file successfully !");
        });
    }
    setThumpbnail(path) {
        this.thumpbnail = 'http://localhost:8080/static/' + path;
        alert("Set Thumpbnail successfully !");
    }
    copyLinkFile(path) {
        var dummy = document.createElement("textarea");
        document.body.appendChild(dummy);
        dummy.setAttribute("id", "myInput1");
        var copyText = document.getElementById("myInput1");
        let fullPath = 'http://localhost:8080/static/' + path;
        copyText.value = fullPath;
        copyText.select();
        copyText.setSelectionRange(0, 99999);
        document.execCommand("copy");
        document.body.removeChild(dummy);
        alert("Copy path file successfully !");
    }
}
