class CourseIndexComponent {
    constructor(service) {
        this.service = service;
        this.data = [];
        this.length = 0;
        this.className = '';
    }
    ngOnInit() {
        this.getData();
    }
    getData() {
        this.service.getData()
            .subscribe((data) => {
            this.data = data;
            this.length = this.data.length;
            if (this.length % 2 == 0)
                this.className = 'col-md-6 col-xs-12 col-sm-12';
            if (this.length % 3 == 0)
                this.className = 'col-md-4 col-xs-12 col-sm-12';
        });
    }
}
