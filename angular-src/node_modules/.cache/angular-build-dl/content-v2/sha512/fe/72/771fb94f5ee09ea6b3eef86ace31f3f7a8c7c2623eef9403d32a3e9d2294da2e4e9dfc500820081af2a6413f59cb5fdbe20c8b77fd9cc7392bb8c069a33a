atch(err => false);\n  }\n  \n  isAuthenticated(user) {\n    return new Promise ((resolve, reject) => {\n      this.http.post(`${this.uri}/isAuthenticated`, user)\n        .subscribe(data => {\n          let rs:any = data;\n          this.cookieService.set('jwt', rs.jwt);\n          resolve(true);\n        },\n         err => {\n           resolve(false);}\n        )\n    }) \n  }\n}\n","import { Component, OnInit } from '@angular/core';\n\n@Component({\n  selector: 'app-contact',\n  templateUrl: './contact.component.html',\n  styleUrls: ['./contact.component.css']\n})\nexport class ContactComponent implements OnInit {\n\n  constructor() { }\n\n  ngOnInit(): void {\n  }\n\n}\n","<section id=\"contact-form\">\n    <div class=\"card\" style=\"margin-top:40px;\">\n        <h3 class=\"card-header text-center\" style=\"background-color: #4285f4\"><b style=\"color: #FFF;\">Đăng kí học lái xe</b></h3>\n        <div class=\"contact\" style=\"margin-left: 10px;margin-right: 10px;margin-top: 20px;\">\n            <div class=\"row\">\n                <div class=\"col-md-9 mb-md-0 mb-5\">\n                    <form id=\"contact-form\" name=\"contact-form\" action=\"mail.php\" method=\"POST\">\n                        <div class=\"row\">\n                            <div class=\"col-md-6\">\n                                <div class=\"md-form mb-0\">\n                                    <input type=\"text\" id=\"name\" name=\"name\" class=\"form-control\" placeholder=\"Họ tên\">\n                                </div>\n                            </div>\n                            <div class=\"col-md-6\">\n                                <div class=\"md-form mb-0\">\n                                    <input type=\"text\" id=\"email\" name=\"email\" class=\"form-control\" placeholder=\"Email\">\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"row\">\n                            <d